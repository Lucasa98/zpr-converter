{
  "name": "zpr-converter",
  "displayName": "zpr converter",
  "description": "Extension para generar configuracion de compilaci√≥n a partir de archivo zpr (Zinjai)",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Lucasa98/zpr-converter"
  },
  "icon": "rsc/zinjaI.png",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "publisher": "LucasSaurin",
  "activationEvents": [],
  "extensionDependencies": [
    "ms-vscode.cpptools"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "zpr-converter.generate",
        "title": "ZPR: generar tasks.json..."
      },
      {
        "command": "zpr-converter.generateFromFile",
        "title": "ZPR: generar tasks.json para este .zpr"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "resourceExtname == .zpr",
          "command": "zpr-converter.generateFromFile",
          "group": "zpr"
        }
      ],
      "editor/title/context": [
        {
          "when": "resourceExtname == .zpr",
          "command": "zpr-converter.generateFromFile",
          "group": "zpr"
        }
      ],
      "explorer/context": [
        {
          "when": "resourceExtname == .zpr",
          "command": "zpr-converter.generateFromFile",
          "group": "zpr"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.102.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
